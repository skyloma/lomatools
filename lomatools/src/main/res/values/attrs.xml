<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="networkimg">
        <attr name="url" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="SegmentedGroup">
        <attr name="corner_radius" format="dimension" />
        <attr name="radio_border_width" format="dimension" />
        <attr name="tint_color" format="color" />
        <attr name="checked_text_color" format="color" />
    </declare-styleable>

     <declare-styleable name="superrecyclerview">
        <attr name="layout_empty" format="reference"/>
        <attr name="layout_moreProgress" format="reference"/>
        <attr name="layout_progress" format="reference"/>
        <attr name="recyclerClipToPadding" format="boolean"/>
        <attr name="recyclerPadding" format="dimension"/>
        <attr name="recyclerPaddingTop" format="dimension"/>
        <attr name="recyclerPaddingBottom" format="dimension"/>
        <attr name="recyclerPaddingLeft" format="dimension"/>
        <attr name="recyclerPaddingRight" format="dimension"/>
        <attr name="scrollbarStyle">
            <flag name="insideOverlay" value="0x0"/>
            <flag name="insideInset" value="0x01000000"/>
            <flag name="outsideOverlay" value="0x02000000"/>
            <flag name="outsideInset" value="0x03000000"/>
        </attr>

        <attr name="mainLayoutId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SwipeLayout">
        <attr name="drag_edge" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
            <enum name="top" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
        <attr name="horizontalSwipeOffset" format="dimension"/>
        <attr name="verticalSwipeOffset" format="dimension"/>
        <attr name="show_mode" format="enum">
            <enum name="lay_down" value="0"/>
            <enum name="pull_out" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="KV">

        <attr name="k" format="string" />
        <attr name="isEdit" format="boolean" />
        <attr name="isCheck" format="boolean" />
        <attr name="isClearButton" format="boolean" />
        <attr name="isUnderLine" format="boolean" />
        <attr name="v" format="string" />
        <attr name="isPwd" format="boolean"></attr>
        <!--<attr name="isClick" format="boolean" />-->
        <attr name="rightIcon" format="reference"></attr>
        <attr name="LeftIcon" format="reference"></attr>
        <attr name="c" format="color" />
        <attr name="max_len" format="integer"></attr>
        <attr name="min_len" format="integer"></attr>
        <attr name="help_text" format="string"></attr>
    </declare-styleable>
    <!--<declare-styleable name="LableEditText">-->

        <!--<attr name="edit_labelString" format="string" />-->
        <!--<attr name="edit_textString" format="string" />-->
       <!-->-->
    <!--</declare-styleable>-->

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
</resources>